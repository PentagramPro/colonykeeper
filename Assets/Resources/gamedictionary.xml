<?xml version="1.0" encoding="windows-1251"?>
<GameDictionary xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Blocks>
  	<Block Name="Bedrock" MaterialName="Bedrock"></Block>
  	<Block Name="Ground" MaterialName="Ground"></Block>
  	<Block Name="Iron Ore" MaterialName="IronOre" Contains="Iron Ore" Freq="0.06"></Block>
  	<Block Name="Ice Ore" MaterialName="IceOre" Contains="Ice" Freq="0.08"></Block>
  </Blocks>
  <Items >
  	<Item Name="Iron Ore" Class="Bulk.Ore"></Item>
  	<Item Name="Ice" Class="Bulk.Ore"></Item>
  	<Item Name="Water" Class="Bulk.Liquid"></Item>
  	<Item Name="Iron Ingot" Class="Good.Ingot"></Item>
  	<Item Name="Iron Plate" Class="Good.Part.Plate"></Item>
  	<Item Name="Iron Beam" Class="Good.Part.Beam"></Item>
  	<Item Name="Iron Wire" Class="Good.Part.Wire"></Item>
