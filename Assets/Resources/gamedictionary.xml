  			<Ingredient Name="Iron Ingot" Quantity="100"></Ingredient>
  		</Ingredients>
  		<Results>
  			<Result Name="Iron Beam" Quantity="100"></Result>
  		</Results>
  	</Recipe>
  	<Recipe Device="Mill" Name = "Roll iron wire">
  		<Ingredients>
  			<Ingredient Name="Iron Ingot" Quantity="100"></Ingredient>
  		</Ingredients>
  		<Results>
  			<Result Name="Iron Wire" Quantity="100"></Result>
  		</Results>
  	</Recipe>
  	
  	<!--       STILL RECIPES             -->
  	<Recipe Device="Still" Name = "Smelt ice">
  		<Ingredients>
  			<Ingredient Name="Ice" Quantity="100"></Ingredient>
  		</Ingredients>
  		<Results>
  			<Result Name="Water" Quantity="40"></Result>
  		</Results>
  	</Recipe>
  	
  	<!--       ASSEMBLER RECIPES        -->
  	<Recipe Device="Assembler" Name = "Assemble servo">
  		<Ingredients>
  			<Ingredient Name="Good.Part.Wire.*" Quantity="200"></Ingredient>
  		</Ingredients>
  		<Results>
  			<Result Name="Servo" Quantity="100"></Result>
  		</Results>
  	</Recipe>
  	<Recipe Device="Assembler" Name = "Assemble missile">
  		<Ingredients>
  			<Ingredient Name="Rocket Fuel" Quantity="100"></Ingredient>
  			<Ingredient Name="Good.Part.Plate.*" Quantity="100"></Ingredient>
  		</Ingredients>
  		<Results>
  			<Result Name="Missile" Quantity="100"></Result>
  		</Results>
  	</Recipe>
  	
  	<!--       CHEMICAL REACTOR RECIPES        -->
  	<Recipe Device="Chemical Reactor" Name = "Produce Rocket Fuel">
  		<Ingredients>
  			<Ingredient Name="Nitrates" Quantity="200"></Ingredient>
  		</Ingredients>
  		<Results>
