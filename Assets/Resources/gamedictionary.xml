  		</Ingredients>
  	</Building>
  	<Building Name="Vehicle Assembler" PrefabName="Vehicle Assembler">
  		<Ingredients>
  			<Ingredient Name="Good.Part.Plate.*" Quantity="100"></Ingredient>
  			<Ingredient Name="Good.Part.Beam.*" Quantity="100"></Ingredient>
  			<Ingredient Name="Iron Wire" Quantity="200"></Ingredient>
  		</Ingredients>
