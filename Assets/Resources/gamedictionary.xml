  		<Ingredients>
  			<Ingredient Name="Good.Part.Plate.*" Quantity="100"></Ingredient>
  			<Ingredient Name="Good.Part.Beam.*" Quantity="100"></Ingredient>
  		</Ingredients>
  	</Building>
  </Buildings>
  <Vehicles>
  	<Vehicle Name="Drone" PrefabName="Drone">
