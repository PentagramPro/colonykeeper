  </Vehicles>
  <Recipes>
  	<!--       FURNACE RECIPES             -->
  	<Recipe Device="Furnace" Name = "Smelt Iron Ore">
  		<Ingredients>
  			<Ingredient Name="Iron Ore" Quantity="200"></Ingredient>
  		</Ingredients>
  		<Results>
  			<Result Name="Iron Ingot" Quantity="100"></Result>
  		</Results>
  	</Recipe>
  	
  	<!--       MILL RECIPES             -->
  	<Recipe Device="Mill" Name = "Roll iron plate">
  		<Ingredients>
  			<Ingredient Name="Iron Ingot" Quantity="200"></Ingredient>
  		</Ingredients>
  		<Results>
  			<Result Name="Iron Plate" Quantity="100"></Result>
  		</Results>
  	</Recipe>
  	<Recipe Device="Mill" Name = "Roll iron beam">
